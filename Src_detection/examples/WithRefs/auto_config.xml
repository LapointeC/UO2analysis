<Configuration>
    <PickleMetaModel>
        <path>References</path>
    </PickleMetaModel>
    
    <Auto>
        <name>bulk_BCC</name>
        <directory>Auto/bulk_BCC</directory>
        <md_format>poscar</md_format>
        <id_atoms>1-20,30-50</id_atoms>
        <model>MCD GMM</model>
        <MCD>
            <mcd_options>
                <nb_bin_histo>150</nb_bin_histo>
                <contamination>0.1</contamination>
            </mcd_options>
        </MCD>
        <GMM>
            <gmm_options>
                <dic_gaussian>
                    <n_components>2</n_components>
                    <covariance_type>diag</covariance_type>
                </dic_gaussian>
            </gmm_options>
        </GMM>
    </Auto>
</Configuration>
